{
  "mcpServers": {
    "context7-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },

    "memory-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },

    "sequential-thinking": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },

    "code-search": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-qdrant"
      ],
      "env": {
        "QDRANT_URL": "http://localhost:6333",
        "COLLECTION_NAME": "macos-app-code-repository",
        "EMBEDDING_MODEL": "sentence-transformers/all-MiniLM-L6-v2",
        "TOOL_STORE_DESCRIPTION": "Store code snippets with descriptions. The 'information' parameter should contain a natural language description of what the code does, while the actual code should be included in the 'metadata' parameter as a 'code' property.",
        "TOOL_FIND_DESCRIPTION": "Search for relevant code snippets using natural language. The 'query' parameter should describe the functionality you're looking for."
      }
    },

    "filesystem": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "./"
      ]
    },

    "git": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-git",
        "--repo",
        "."
      ]
    },

    "apple-docs-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@kimsungwhee/apple-docs-mcp@latest"
      ]
    },

    "xcodemcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "xcodemcp@latest"
      ],
      "env": {
        "LOG_LEVEL": "INFO"
      }
    },

    "RepoPrompt": {
      "type": "stdio",
      "command": "/Users/danielfinley/RepoPrompt/repoprompt_cli",
      "args": []
    },

    "octocode-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "octocode-mcp"
      ]
    },

    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--project",
        "."
      ]
    },

    "linear": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.linear.app/sse"
      ]
    }
  }
}