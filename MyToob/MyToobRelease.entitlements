<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- App Sandbox (Required for App Store) -->
	<key>com.apple.security.app-sandbox</key>
	<true/>

	<!-- Hardened Runtime (Required for App Store) -->
	<key>com.apple.security.hardened-runtime</key>
	<true/>

	<!-- Network Access (Required for YouTube Data API) -->
	<key>com.apple.security.network.client</key>
	<true/>

	<!-- User Selected Files (Required for local video import) -->
	<key>com.apple.security.files.user-selected.read-write</key>
	<true/>

	<!-- Keychain Access Groups (Required for OAuth token storage) -->
	<key>keychain-access-groups</key>
	<array>
		<string>$(AppIdentifierPrefix)finley.MyToob</string>
	</array>

	<!-- CloudKit & App Groups: REMOVED (require paid Apple Developer account)
	     Add these back when you enroll in Apple Developer Program:
	     - com.apple.developer.icloud-container-identifiers
	     - com.apple.developer.icloud-services
	     - com.apple.security.application-groups
	-->
</dict>
</plist>
